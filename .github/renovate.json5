{
  "extends": [
    "config:best-practices",
    "docker:enableMajor",
    ":disableRateLimiting",
    ":semanticCommits",
    ":automergeDigest",
    ":automergeBranch",
    "github>auricom/home-ops//.github/renovate/allowedVersions.json5",
    "github>auricom/home-ops//.github/renovate/autoMerge.json5",
    "github>auricom/home-ops//.github/renovate/commitMessage.json5",
